{
  "name": "giftforyou.idn",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "scripts": {
    "start": "NODE_OPTIONS='--max-http-header-size=16384 --no-deprecation' react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:server": "DOTENV_CONFIG_PATH=.env.development node --max-http-header-size=16384 --no-deprecation -r ts-node/register/transpile-only -r dotenv/config ./node_modules/.bin/ts-node-dev --respawn --transpile-only src/server/server.ts",
    "check-env": "node scripts/check-env.js",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node --max-http-header-size=16384 --no-deprecation dist/server/server.js",
    "seed": "ts-node src/seed/seed.ts",
    "dev": "concurrently -n WEB,API -c cyan,magenta \"npm start\" \"npm run dev:server\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/react-window": "^1.8.8",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "heic-convert": "^2.1.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "mongoose": "^7.6.1",
    "multer": "^2.0.2",
    "rc-slider": "^11.1.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^10.0.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-window": "^2.2.3",
    "sharp": "^0.34.5",
    "swiper": "^12.0.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/heic-convert": "^2.1.0",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.5.9",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "yaml": "^2.8.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
